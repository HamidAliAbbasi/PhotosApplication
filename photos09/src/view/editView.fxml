<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<TitledPane alignment="CENTER" animated="false"
	collapsible="false" maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="137.0"
	prefWidth="185.0" text="Edit Album"
	xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controllers.editController">
	<content>
		<AnchorPane minHeight="0.0" minWidth="0.0"
			prefHeight="42.0" prefWidth="0.0">
			<children>
				<TextField fx:id="newName" layoutX="21.0" layoutY="26.0"
					prefHeight="28.0" prefWidth="141.0" promptText="Edit Name">
					<font>
						<Font size="11.0" />
					</font>
				</TextField>
				<Button fx:id="edit" layoutX="10.0" layoutY="65.0"
					mnemonicParsing="false" onAction="#edit" text="Edit Album" />
				<Button fx:id="close" layoutX="103.0" layoutY="65.0"
					mnemonicParsing="false" onAction="#close" prefHeight="26.0"
					prefWidth="72.0" text="Close">
					<font>
						<Font name="System Font" size="12.0" />
					</font>
				</Button>
			</children>
		</AnchorPane>
	</content>
	<font>
		<Font size="14.0" />
	</font>
</TitledPane>
