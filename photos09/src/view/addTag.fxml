<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<TitledPane alignment="CENTER" animated="false"
	collapsible="false" maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="166.0"
	prefWidth="222.0" text="Add Tag"
	xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controllers.addTagController">
	<content>
		<AnchorPane minHeight="0.0" minWidth="0.0"
			prefHeight="145.0" prefWidth="212.0">
			<children>
				<TextField fx:id="type" layoutX="25.0" layoutY="14.0"
					promptText="Type">
					<font>
						<Font name="Arial" size="11.0" />
					</font>
				</TextField>
				<Button layoutX="25.0" layoutY="94.0" mnemonicParsing="false"
					onAction="#addTag" text="Add Tag">
					<font>
						<Font name="Arial" size="12.0" />
					</font>
				</Button>
				<Button layoutX="117.0" layoutY="94.0"
					mnemonicParsing="false" onAction="#close" prefHeight="26.0"
					prefWidth="72.0" text="Finished">
					<font>
						<Font name="Arial" size="12.0" />
					</font>
				</Button>
				<TextField fx:id="value" layoutX="25.0" layoutY="54.0"
					promptText="Value">
					<font>
						<Font name="Arial" size="12.0" />
					</font>
				</TextField>
			</children>
		</AnchorPane>
	</content>
	<font>
		<Font name="Arial" size="11.0" />
	</font>
</TitledPane>
