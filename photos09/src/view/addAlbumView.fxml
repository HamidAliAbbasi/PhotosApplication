<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<TitledPane alignment="CENTER" animated="false"
	collapsible="false" maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="137.0"
	prefWidth="185.0" text="Add Album"
	xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controllers.addAlbumController">
	<content>
		<AnchorPane minHeight="0.0" minWidth="0.0"
			prefHeight="42.0" prefWidth="0.0">
			<children>
				<TextField fx:id="albumName" layoutX="10.0" layoutY="14.0"
					prefHeight="34.0" prefWidth="156.0" promptText="Album Name">
					<font>
						<Font size="12.0" />
					</font>
				</TextField>
				<Button layoutX="10.0" layoutY="56.0" mnemonicParsing="false"
					onAction="#add" prefHeight="19.0" prefWidth="87.0" text="Add Album">
					<font>
						<Font size="11.0" />
					</font>
				</Button>
				<Button layoutX="104.0" layoutY="56.0"
					mnemonicParsing="false" onAction="#close" prefHeight="20.0"
					prefWidth="65.0" text="Close">
					<font>
						<Font size="11.0" />
					</font>
				</Button>
			</children>
		</AnchorPane>
	</content>
	<font>
		<Font size="17.0" />
	</font>
</TitledPane>
