<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<TitledPane fx:id="titlePane" alignment="CENTER"
	animated="false" collapsible="false" contentDisplay="CENTER"
	prefHeight="644.0" prefWidth="395.0" text="Photos in &quot;&quot;"
	xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controllers.specificAlbumViewController">
	<content>
		<AnchorPane minHeight="0.0" minWidth="0.0"
			pickOnBounds="false" prefHeight="611.0" prefWidth="443.0">
			<children>
				<ListView fx:id="photosList" layoutX="28.0" layoutY="155.0"
					prefHeight="131.0" prefWidth="331.0" />
				<Button fx:id="add" layoutX="253.0" layoutY="299.0"
					mnemonicParsing="false" onAction="#add" prefHeight="19.0"
					prefWidth="88.0" text="Add">
					<font>
						<Font size="10.0" />
					</font>
				</Button>
				<Button fx:id="delete" layoutX="26.0" layoutY="336.0"
					mnemonicParsing="false" onAction="#delete" prefHeight="22.0"
					prefWidth="73.0" text="Delete">
					<font>
						<Font size="11.0" />
					</font>
				</Button>
				<Button fx:id="back" layoutX="26.0" layoutY="299.0"
					mnemonicParsing="false" onAction="#back" prefHeight="18.0"
					prefWidth="74.0" text="Back">
					<font>
						<Font size="11.0" />
					</font>
				</Button>

				<Button fx:id="copy" layoutX="132.0" layoutY="336.0"
					mnemonicParsing="false" onAction="#copy" prefHeight="22.0"
					prefWidth="101.0" text="Copy">
					<font>
						<Font size="11.0" />
					</font>
				</Button>
				<Button fx:id="quit" layoutX="285.0" layoutY="371.0"
					mnemonicParsing="false" onAction="#quit" prefHeight="22.0"
					prefWidth="56.0" text="Quit">
					<font>
						<Font size="11.0" />
					</font>
				</Button>
				<TextField fx:id="captions" editable="false"
					layoutX="112.0" layoutY="407.0" prefHeight="29.0" prefWidth="235.0" />
				<Label layoutX="39.0" layoutY="409.0" text="Caption:" />

				<Label layoutX="46.0" layoutY="453.0" text="Date:" />

				<TextField fx:id="dateField" editable="false"
					layoutX="112.0" layoutY="447.0" prefHeight="29.0" prefWidth="235.0" />

				<ListView fx:id="tagsList" layoutX="114.0" layoutY="494.0"
					prefHeight="66.0" prefWidth="231.0" />
				<Label layoutX="45.0" layoutY="519.0" text="Tags:" />
				<Button layoutX="25.0" layoutY="371.0"
					mnemonicParsing="false" onAction="#edit" prefHeight="21.0"
					prefWidth="75.0" text="Edit Caption">
					<font>
						<Font size="11.0" />
					</font>
				</Button>
				<Button fx:id="addTag" layoutX="112.0" layoutY="371.0"
					mnemonicParsing="false" onAction="#addTag" prefHeight="18.0"
					prefWidth="65.0" text="Add Tag">
					<font>
						<Font size="11.0" />
					</font>
				</Button>
				<Button fx:id="delTag" layoutX="193.0" layoutY="371.0"
					mnemonicParsing="false" onAction="#delTag" prefHeight="22.0"
					prefWidth="73.0" text="Delete Tag">
					<font>
						<Font size="11.0" />
					</font>
				</Button>
				<Button fx:id="move" layoutX="253.0" layoutY="336.0"
					mnemonicParsing="false" onAction="#move" prefHeight="22.0"
					prefWidth="89.0" text="Move">
					<font>
						<Font size="11.0" />
					</font>
				</Button>
				<ImageView fx:id="imageShow" fitHeight="146.0"
					fitWidth="192.0" layoutX="93.0" pickOnBounds="true"
					preserveRatio="true" />
				<Button fx:id="prev" layoutX="9.0" layoutY="53.0"
					mnemonicParsing="false" onAction="#prev" prefHeight="18.0"
					prefWidth="74.0" text="Previous">
					<font>
						<Font size="11.0" />
					</font>
				</Button>
				<Button fx:id="next" layoutX="294.0" layoutY="53.0"
					mnemonicParsing="false" onAction="#next" prefHeight="18.0"
					prefWidth="74.0" text="Next">
					<font>
						<Font size="11.0" />
					</font>
				</Button>
			</children>
		</AnchorPane>
	</content>
	<font>
		<Font name="Arial" size="17.0" />
	</font>
</TitledPane>
