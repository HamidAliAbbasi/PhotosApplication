<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<TitledPane alignment="CENTER" animated="false"
	collapsible="false" maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="160.0"
	prefWidth="309.0" text="Add Photo"
	xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controllers.addPhotoController">
	<content>
		<AnchorPane minHeight="0.0" minWidth="0.0"
			prefHeight="234.0" prefWidth="307.0">
			<children>
				<TextField fx:id="path" editable="false" layoutX="52.0"
					layoutY="14.0" prefHeight="26.0" prefWidth="206.0"
					promptText="Photo Path">
					<font>
						<Font name="Arial Bold" size="12.0" />
					</font>
				</TextField>
				<Button layoutX="36.0" layoutY="80.0" mnemonicParsing="false"
					onAction="#add" text="Add" />
				<Button layoutX="213.0" layoutY="92.0"
					mnemonicParsing="false" onAction="#close" prefHeight="26.0"
					prefWidth="72.0" text="Close">
					<font>
						<Font name="Arial" size="12.0" />
					</font>
				</Button>
				<TextField fx:id="cap" layoutX="52.0" layoutY="45.0"
					prefHeight="26.0" prefWidth="206.0" promptText="Caption">
					<font>
						<Font name="Arial" size="12.0" />
					</font>
				</TextField>
				<Button layoutX="80.0" layoutY="80.0" mnemonicParsing="false"
					onAction="#getUrl" text="Browse" />
			</children>
		</AnchorPane>
	</content>
	<font>
		<Font name="Arial" size="12.0" />
	</font>
</TitledPane>
